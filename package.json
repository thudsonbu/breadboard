    "coverage": "nyc npm run test",
    "dev:tsc": "tsc --watch -p .",
    "dev:serve": "nodemon -e js -w lib lib/index.js",
    "dev": "run-p dev:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thudsonbu/integrations.git"
  },
  "keywords": [
    "habitica",
    "todoist",
    "notion",
    "hapi",
    "axios",
    "mocha",
    "chai",
    "rewire",
    "sinon",
    "typescript"
  ],
  "author": "Thomas Hudson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thudsonbu/integrations/issues"
  },
  "homepage": "https://github.com/thudsonbu/integrations#readme",
  "dependencies": {
    "@hapi/hapi": "^20.2.2",
    "@starryinternet/jobi": "^1.2.0",
    "axios": "^0.27.2"
  },
  "devDependencies": {
    "@types/hapi": "^18.0.7",
    "@types/hapi__hapi": "^20.0.13",
    "@types/node": "^18.11.3",
    "chai": "^4.3.6",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "rewire": "^6.0.0",
    "sinon": "^14.0.0",
    "typescript": "^4.8.4"
  }
