{
  "name": "integrations",
  "version": "0.2.0",
  "description": "Integrations for Todoist and Habitica.",
  "main": "lib/index.js",
  "type": "commonjs",
  "scripts": {
    "start": "NODE_DEBUG=info node index.js",
    "mocha": "NODE_DEBUG=info NODE_ENV=test mocha --recursive",
